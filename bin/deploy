#! /usr/bin/env bash

# DOC: Deploy an image. Usage: bin/deploy --tag=<image tag> 
# Optional params:
#   --config=<config_file> to override config file

source bin/lib.sh
source ${CONFIG_FILE}

if [[ ! -z "${USE_DOCKER}" ]]; then
    COMMAND="deploy"
    exec "bin/lib/run-from-docker"
else 
    CMD_NAME=deploy checkout::exec_delegated_command "$@"
fi
